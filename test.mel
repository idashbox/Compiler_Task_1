class Fibonacci {
    int a = 0;
    int b = 1;
    
    int next() {
        int temp = b;
        b = a + b;
        a = temp;
        return a;
    }
}

// Создаем объект для вычисления чисел Фибоначчи
Fibonacci fib = new Fibonacci();

// Выводим первые 10 чисел Фибоначчи
println(fib.a);  // 0

fib.next();
println(fib.a);  // 1

fib.next();
println(fib.a);  // 1

fib.next();
println(fib.a);  // 2

fib.next();
println(fib.a);  // 3

